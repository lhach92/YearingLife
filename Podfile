# Uncomment this line to define a global platform for your project
platform :ios, ‘9.0’
source 'https://github.com/CocoaPods/Specs.git'

inhibit_all_warnings!

target 'YearningLife' do

pod 'AVOSCloud', '~> 4.4.0'            # 数据存储、短信、云引擎调用等基础服务模块
pod 'AVOSCloudIM', '~> 4.4.0'          # 实时通信模块
pod 'AVOSCloudCrashReporting', '~> 4.4.0' # 错误报告模块

end